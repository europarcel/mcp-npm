{
  "name": "@europarcel/mcp",
  "version": "1.1.11",
  "description": "MCP server for Europarcel API - Enable AI assistants to interact with shipping and logistics services",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "europarcel-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:dev": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src",
    "prepare": "npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "europarcel",
    "shipping",
    "logistics",
    "api",
    "ai",
    "claude"
  ],
  "author": "Europarcel SRL",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "axios": "^1.12.2",
    "dotenv": "^16.4.7",
    "winston": "^3.18.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!dist/**/*.map",
    "README.MD",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/europarcel/mcp-npm.git"
  },
  "bugs": {
    "url": "https://github.com/europarcel/mcp-npm/issues"
  },
  "homepage": "https://www.europarcel.com"
}
